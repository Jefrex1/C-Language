#include <stdio.h>

int main() {
    int i, p, g, res, res1 = 1;
    int m[2][3] = { 
        {6, 7, 5},
        {6, 4, 3}
    };

        for (g=0;g<3;g++) {
            res = m[0][g] + m[1][g];

            printf("Summa stovpcia %d: %d\n",g+1, res);
            res = 0;
        }

    for (i=0;i<2;i++) {
        for (p=0;p<3;p++) {
            res1 = res1 * m[i][p];

        }
        printf("Dobutok riadka %d: %d\n",i+1, res1);
        res1 = 1;
    }
}
